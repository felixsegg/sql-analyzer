<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="200.0" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <HBox alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="NEVER">
      <Label fx:id="headerLabel" style="-fx-font-size: 18px;" />
      <Label fx:id="helpControl" alignment="CENTER" minWidth="15.0" style="-fx-background-color: lightblue;" text="?" />
      <Region HBox.hgrow="ALWAYS" />
      <Button fx:id="refreshBtn" mnemonicParsing="false" text="Refresh" />
   </HBox>
   <Separator prefWidth="200.0" />
   <HBox alignment="CENTER_RIGHT" spacing="10.0">
      <Button fx:id="deleteBtn" disable="true" mnemonicParsing="false" text="Delete" />
      <Button fx:id="addBtn" mnemonicParsing="false" text="Add new..." />
   </HBox>
   <TitledPane text="Filter">
      <HBox fx:id="filterNodeContainer" nodeOrientation="RIGHT_TO_LEFT" spacing="10.0" />
   </TitledPane>
   <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
</VBox>
